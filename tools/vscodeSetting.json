{
  "editor.fontSize": 18,
  "git.ignoreLegacyWarning": true,
  "workbench.colorTheme": "Monokai",
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "editor.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      {
          "language": "vue",
          "autoFix": true
      },
      {
          "language": "typescript",
          "autoFix": true
      },
      // {
      //     "language": "typescriptreact",
      //     "autoFix": true
      // }
  ],
}